{
    "name": "menumbing/menumbing",
    "description": "The hyperf framework components",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Menumbing\\Contract\\": "contract",
            "Menumbing\\HttpClient\\": "http-client/src",
            "Menumbing\\Orm\\": "orm/src",
            "Menumbing\\Resource\\": "resource/src",
            "Menumbing\\Signature\\": "signature/src",
            "Menumbing\\Tracer\\": "tracer/src",
            "Menumbing\\GraphQL\\": "graphql/src"
        }
    },
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "hyperf/di": "3.1.*",
        "hyperf/event": "^3.1",
        "hyperf/framework": "^3.1",
        "hyperf/guzzle": "^3.1",
        "hyperf/server": "^3.1",
        "hyperf/db-connection": "^3.1",
        "hyperf/cache": "^3.1",
        "hyperf/config": "^3.1",
        "hyperf/pipeline": "^3.1",
        "hyperf/http-server": "^3.1",
        "symfony/http-foundation": "^6.4",
        "hyperf/resource": "^3.1",
        "ramsey/uuid": "^4.7",
        "menumbing/auth": "^1.0",
        "hyperf/exception-handler": "^3.1",
        "hyperf/validation": "^3.1",
        "hyperf/tracer": "^3.1",
        "psr/container": "^2.0",
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "thecodingmachine/graphqlite": "^6.2",
        "hyperf/http-message": "~3.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "dev-master"
    }
}
